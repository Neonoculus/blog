<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.example.blog.mapper.CommandMapper">

    <insert id="insert" parameterType="org.example.blog.model.Command">
        INSERT INTO command (c_id, c_content, c_time, u_id)
        VALUES (#{c_id}, #{c_content}, #{c_time}, #{u_id})
    </insert>

</mapper>